#include<iostream>
#include<vector>

int main(){

  int n , t;

  std::vector <int> v;

  std::cout<<"type a number n:"<<std::endl;
  std::cin>> n;

  std::cout<<"type a number t:"<<std::endl;
  std::cin>> t;
  std::cout<<std::endl;
  
  for(int i=0, k; i<n; i++){
    
    std::cout<<"type a number that goes in the vector:"<<std::endl ;
    std::cin>> k;

    v.push_back(k);
  }
  
  std::cout<<std::endl;
  std::cout<< "the result is: "<< std::endl;
  
  for(int j=0; j<t; j++){
    
    std::cout<< v[0 + j] << std::endl;
  }

